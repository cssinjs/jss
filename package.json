{
  "name": "jss",
  "description": "A lib for generating Style Sheets with JavaScript.",
  "version": "5.5.6",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/jss.git"
  },
  "keywords": [
    "jss",
    "style",
    "sheet",
    "stylesheet",
    "css",
    "components",
    "composable",
    "css in js",
    "css-in-js"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=5.0.0 <7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.0.0",
    "es5-shim": "^4.3.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-jss": "^2.3.0",
    "eslint-plugin-flowtype": "^2.17.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "expect.js": "^0.3.1",
    "flow-bin": "0.33.0",
    "json-loader": "^0.5.4",
    "jss-compose": "^1.0.1",
    "jss-expand": "^1.5.1",
    "jss-extend": "^2.0.1",
    "jss-nested": "^2.0.0",
    "karma": "^1.1.1",
    "karma-benchmark": "^0.6.0",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-browserstack-launcher": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "lint-staged": "^3.0.1",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "react": ">=14.0.0 <=15.1.0",
    "react-deep-force-update": "^2.0.1",
    "react-dom": "^15.3.2",
    "react-jss": "^4.1.3",
    "rimraf": "^2.5.4",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "all": "npm run lint && npm run flow && npm run test && npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:max && npm run build:min && npm run build:tests",
    "build:lib": "cross-env VERSION=`npm run version -s` && VERSION=$VERSION babel src --out-dir lib",
    "build:max": "cross-env NODE_ENV=development webpack src/index.js dist/jss.js",
    "build:min": "cross-env NODE_ENV=production webpack src/index.js dist/jss.min.js",
    "build:tests": "cross-env NODE_ENV=test webpack tests/index.js tmp/tests.js",
    "clean": "rimraf '{lib,dist,tmp}/*'",
    "flow": "flow --show-all-errors",
    "lint": "eslint ./src ./tests ./benchmark ./*.js",
    "lint:staged": "lint-staged && npm run flow",
    "prepublish": "npm run all",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "posttest": "[ -z \"$TRAVIS\" ] || codecov",
    "codecov": "codecov",
    "bench": "cross-env BENCHMARK=true npm run test",
    "version": "node -e \"console.log(require('./package.json').version)\""
  },
  "dependencies": {
    "is-in-browser": "^1.0.2",
    "murmurhash-js": "^1.0.0",
    "warning": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
