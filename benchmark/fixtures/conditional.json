{
  "@media (min-width: 1024px)": {
    "modal": {
      "position": "fixed",
      "top": 0,
      "right": 0,
      "bottom": 0,
      "left": 0,
      "z-index": 1040,
      "display": "none",
      "overflow": "hidden",
      "outline": 0
    }
  },
  "@supports (display: flex)": {
    "legend": {
      "display": "flex",
      "width": "100%",
      "padding": 0,
      "margin-bottom": "20px",
      "font-size": "21px",
      "line-height": "inherit",
      "color": "#333",
      "border": 0,
      "border-bottom": "1px solid #e5e5e5"
    }
  }
}
