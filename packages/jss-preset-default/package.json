{
  "name": "jss-preset-default",
  "description": "Default preset for JSS with selected plugins.",
  "version": "3.0.0",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/jss-preset-default.git"
  },
  "keywords": [
    "jss",
    "preset",
    "default"
  ],
  "scripts": {
    "all": "npm run lint && npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:max && npm run build:min",
    "clean": "rimraf ./lib/*",
    "build:lib": "babel src --out-dir lib",
    "build:max": "NODE_ENV=development webpack src/index.js dist/jss-preset-default.js",
    "build:min": "NODE_ENV=production webpack src/index.js dist/jss-preset-default.min.js",
    "lint": "eslint ./src",
    "prepublish": "npm run all"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "jss": "^8.0.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "jss": "^8.0.0"
  },
  "dependencies": {
    "jss-camel-case": "^5.0.0",
    "jss-compose": "^4.0.0",
    "jss-default-unit": "^7.0.0",
    "jss-expand": "^4.0.0",
    "jss-extend": "^5.0.0",
    "jss-global": "^2.0.0",
    "jss-nested": "^5.0.0",
    "jss-props-sort": "^5.0.0",
    "jss-vendor-prefixer": "^6.0.0"
  }
}
