{
  "presets": [
    ["es2015", {"modules": false}],
    "stage-0"
  ],
  "plugins": [
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals",
    "transform-flow-strip-types",
    "inline-version",
    "external-helpers"
  ],
  "env": {
    "test": {
      "plugins": [
        "rewire"
      ]
    }
  }
}
